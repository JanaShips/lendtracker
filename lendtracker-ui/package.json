{
  "name": "lendtracker-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate-icons": "node scripts/generate-icons.cjs",
    "generate-android-icons": "node scripts/generate-android-icons.cjs",
    "cap:init": "npx cap init LendTracker com.lendtracker.app --web-dir dist",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "build:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "build:android": "npm run build && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build && npx cap sync android && cd android && gradlew.bat assembleDebug",
    "android:release": "npm run build && npx cap sync android && cd android && gradlew.bat assembleRelease"
  },
  "dependencies": {
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3"
  },
  "devDependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.8",
    "@capacitor/cli": "^5.6.0",
    "@capacitor/core": "^5.6.0",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.3.6",
    "terser": "^5.26.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.8"
  }
}
